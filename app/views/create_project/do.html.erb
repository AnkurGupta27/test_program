<table>
  <th>
    Problem
  </th>
  <th>
    Upload Image
  </th>
  <tr>
    <td>
       <%= @problem.description %>
    </td>
    <td>
      <%= form_tag url_for("/create_project/upload") do %>
          <%=hidden_field_tag "problem_id", @problem.id %>
          <%= submit_tag "Upload", :class => "btn btn-info btn-small" %>
      <% end %>
    </td>
  </tr>
</table>